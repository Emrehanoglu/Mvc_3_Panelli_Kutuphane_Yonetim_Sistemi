@using MvcKutuphane.Models.Entity
@model List<TblHareket>
@{
    ViewBag.Title = "UyeKitapGecmis";
    Layout = "~/Views/Shared/Layout.cshtml";
}
<h2>@ViewBag.dgr1 @ViewBag.dgr2</h2>
<br />
<table class="table table-hover">
    <tr>
        <th>ID</th>
        <th>Kitaplar</th>
    </tr>
    @foreach (var u in Model)
    {
        <tr>
            <td>@u.ID</td>
            <td>@u.TblKitap.AD</td>
        </tr>
    }
</table>
<a href="/Uye/Index/" class="btn btn-group" style="background-color:#000000;color:white">Geri Dön</a>