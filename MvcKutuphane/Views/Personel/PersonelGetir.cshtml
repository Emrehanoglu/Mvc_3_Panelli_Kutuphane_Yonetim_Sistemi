@model MvcKutuphane.Models.Entity.TblPersonel
@{
    ViewBag.Title = "PersonelGetir";
    Layout = "~/Views/Shared/Layout.cshtml";
}

@using (Html.BeginForm("PersonelGuncelle", "Personel", FormMethod.Post))
{
    <div style="margin-bottom:15px">
        <label>Personel ID</label>
        @Html.TextBoxFor(x => x.ID, new { @readonly = "readonly", @class = "form-control" })
        <label>Personel</label>
        @Html.TextBoxFor(x => x.PERSONEL, new { @class = "form-control" })
    </div>
    <div>
        <button class="btn btn-info">Personel Güncelle</button>
    </div>
}

